{
  "name": "jakanz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "npm-run-all -p tailwind:watch start",
    "start": "react-scripts start",
    "build": "npm run tailwind:build && npm run react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:watch": "tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./src/index.css -o ./src/tailwind.css --minify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "html-webpack-plugin": "^5.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
